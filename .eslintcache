runtime}\n        revenue={this.state.revenue}\n        budget={this.state.budget}\n        genres={this.state.genres}\n        tagline={this.state.tagline}\n        date={this.state.date}\n/>\n      </section>\n    )\n  }\n}\n\nexport default IndividualView\n","/Users/drewbradley/mod3/rancid-tomatillos/src/components/Movie.js",[],"/Users/drewbradley/mod3/rancid-tomatillos/src/components/Footer.js",["43"],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":4,"column":24,"nodeType":"50","messageId":"51","endLine":4,"endColumn":32},{"ruleId":"52","severity":1,"message":"53","line":8,"column":7,"nodeType":"54","endLine":8,"endColumn":73},{"ruleId":"48","severity":1,"message":"55","line":5,"column":9,"nodeType":"50","messageId":"51","endLine":5,"endColumn":16},{"ruleId":"52","severity":1,"message":"53","line":18,"column":7,"nodeType":"54","endLine":18,"endColumn":51},"no-native-reassign",["56"],"no-negated-in-lhs",["57"],"no-unused-vars","'Redirect' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","'NavLink' is defined but never used.","no-global-assign","no-unsafe-negation"]